{
    "name": "Project-zeus-frontend",
    "version": "1.0.0",
    "description": "React boilerplate for future projects",
    "main": "index.js",
    "author": "Niilo Jaakkola <niilo.jaakkola@icloud.com>",
    "license": "MIT",
    "scripts": {
        "start": "yarn run dev",
        "setup": "yarn install",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js --open",
        "lint": "eslint src",
        "format": "yarn run prettier --write",
        "validate": "yarn run lint && yarn run prettier --list-different",
        "prettier": "prettier \"**/*.+(js|jsx|json|ts|tsx|css|scss|md|mdx|graphql)\"",
        "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --color -p --progress --hide-modules --display-optimization-bailout"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint",
        "**/*.+(js|jsx|json|ts|tsx|css|scss|md|mdx|graphql)": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "clean-webpack-plugin": "^1.0.1",
        "css-loader": "^2.1.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.12.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.0",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "prettier": "^1.16.1",
        "redux-devtools": "^3.5.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.2",
        "url-loader": "^1.1.2",
        "webpack": "^4.28.3",
        "webpack-cli": "^3.2.0",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-dev-server": "^3.1.14",
        "webpack-hot-middleware": "^2.24.3"
    },
    "dependencies": {
        "@material-ui/core": "^3.9.3",
        "axios": "^0.18.0",
        "connected-react-router": "^6.1.0",
        "cross-env": "^5.2.0",
        "dotenv-webpack": "^1.6.0",
        "history": "^4.7.2",
        "moment": "^2.24.0",
        "prop-types": "^15.6.2",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-helmet": "^5.2.0",
        "react-redux": "^6.0.0",
        "react-router-dom": "^4.3.1",
        "react-transition-group": "^2.5.2",
        "redux": "^4.0.1",
        "redux-axios-middleware": "^4.0.0",
        "redux-form": "^8.1.0",
        "redux-persist": "^5.10.0",
        "redux-thunk": "^2.3.0"
    }
}
